rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Users can only upload/download their own files
    match /users/{uid}/{allPaths=**} {
      allow read, write: if request.auth.uid == uid;
    }

    // Deny all other access
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
